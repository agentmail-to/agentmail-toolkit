{
    "name": "agentmail-toolkit",
    "version": "0.1.30",
    "description": "AgentMail Toolkit",
    "scripts": {
        "build": "tsup",
        "lint": "eslint .",
        "prettier": "prettier --write .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "AgentMail",
        "Toolkit"
    ],
    "author": "AgentMail <support@agentmail.cc>",
    "license": "MIT",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/agentmail-to/agentmail-toolkit.git"
    },
    "packageManager": "pnpm@10.6.4",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./ai-sdk": {
            "types": "./dist/ai-sdk.d.ts",
            "import": "./dist/ai-sdk.mjs",
            "require": "./dist/ai-sdk.js"
        },
        "./langchain": {
            "types": "./dist/langchain.d.ts",
            "import": "./dist/langchain.mjs",
            "require": "./dist/langchain.js"
        },
        "./mcp": {
            "types": "./dist/mcp.d.ts",
            "import": "./dist/mcp.mjs",
            "require": "./dist/mcp.js"
        }
    },
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@types/node": "^24.0.1",
        "@types/pdf-parse": "^1.1.4",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "globals": "^16.2.0",
        "prettier": "3.5.3",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.0"
    },
    "dependencies": {
        "agentmail": "^0.1.8",
        "file-type": "^20.0.0",
        "mammoth": "^1.8.0",
        "pdf-parse": "^1.1.1",
        "zod": "^3.25.63"
    },
    "peerDependencies": {
        "@langchain/core": "^0.3.58",
        "@modelcontextprotocol/sdk": "^1.12.1",
        "ai": "^4.3.16"
    }
